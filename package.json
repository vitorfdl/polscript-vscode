{
    "name": "polscript",
    "displayName": "Polscript Language Support",
    "description": "polscript (ultima server) colorizer support for vscode",
    "version": "1.1.0",
    "publisher": "vitorfdl",
    "icon": "favicon.ico",
    "engines": {
        "vscode": "^1.0.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "polscript",
                "aliases": [
                    "polscript",
                    "polscript"
                ],
                "extensions": [
                    ".src",
                    ".inc",
                    ".em"
                ],
                "configuration": "./polscript.configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "polscript",
                "scopeName": "source.src",
                "path": "./syntaxes/polscript.tmLanguage"
            }
        ]
    }
}