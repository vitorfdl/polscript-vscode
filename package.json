{
	"name": "polscript",
	"displayName": "Polscript Language Support",
	"description": "polscript (ultima server) colorizer support for vscode",
	"version": "2.1.0",
	"publisher": "vitorfdl",
	"icon": "favicon.ico",
	"engines": {
		"vscode": "^1.0.0"
	},
	"categories": [
		"Programming Languages"
	],
	"contributes": {
		"languages": [
			{
				"id": "polscript",
				"aliases": [
					"polscript"
				],
				"extensions": [
					".src",
					".inc",
					".em"
				],
				"configuration": "./polscript.configuration.json"
			},
			{
				"id": "cfgpol",
				"aliases": [
					"cfgpol"
				],
				"extensions": [
					".cfg"
				],
				"configuration": "./cfgpol.configuration.json"
			}
		],
		"grammars": [
			{
				"language": "polscript",
				"scopeName": "source.polscript",
				"path": "./syntaxes/polscript.tmLanguage"
			},
			{
				"language": "cfgpol",
				"scopeName": "source.cfgpol",
				"path": "./syntaxes/cfg.tmLanguage"
			}
		]
	}
}